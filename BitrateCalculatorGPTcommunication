    <option value="384000">384 kHz</option>
    <option value="512000">512 kHz</option>
    <option value="705600">705.6 kHz</option>
    <option value="768000">768 kHz</option>
    <option value="1024000">1024 kHz</option>
    <option value="2822400">2.8224 Mhz (DSD64)</option>
    <option value="5644800">5.6448 Mhz (DSD128)</option>
    <option value="11289600">11.2896 Mhz (DSD256)</option>
    <option value="22579200">22.5792 Mhz (DSD512)</option>
    <option value="45158400">45.1584 Mhz (DSD1024)</option>
    <option value="90316800">90.3168 Mhz (DSD2048)</option>
    <option value="4300000000">4.3 GHz (maximum)</option>
</select>

<label for="bitDepth">Bit Depth (Word Length):</label>
<select id="bitDepth">
    <option value="1">1-bit</option>
    <option value="2">2-bit</option>
    <option value="3">3-bit</option>
    <option value="4">4-bit</option>
    <option value="6">6-bit</option>
    <option value="8">8-bit</option>
    <option value="10">10-bit</option>
    <option value="12">12-bit</option>
    <option value="16" selected>16-bit (CD)</option>
    <option value="20">20-bit</option>
    <option value="24">24-bit</option>
    <option value="32">32-bit</option>
    <option value="64">64-bit</option>
    <option value="128">128-bit (maximum)</option>
</select>

<label for="channels">Channels:</label>
<select id="channels">
    <option value="1">1 (mono)</option>
    <option value="2">2 (stereo)</option>
    <option value="3">3</option>
    <!-- Add other options here -->
    <option value="65535">65535 (maximum)</option>
</select>

<button onclick="calculateBitrate()">Calculate</button>
<button onclick="resetInputs()">Reset</button>

<div id="output">Bitrate: 1411.2 kbps</div>

<script>
function calculateBitrate() {
    var sampleRate = parseFloat(document.getElementById("sampleRate").value);
    var bitDepth = parseFloat(document.getElementById("bitDepth").value);
    var channels = parseFloat(document.getElementById("channels").value);
    
    var bitrate = (sampleRate * bitDepth * channels) / 1000;
    
    document.getElementById("output").textContent = "Bitrate: " + bitrate.toFixed(1) + " kbps";
}

function resetInputs() {
    document.getElementById("sampleRate").value = "44100";
    document.getElementById("bitDepth").value = "16";
    document.getElementById("channels").value = "2";
    
    calculateBitrate();
}

calculateBitrate(); // Calculate bitrate initially
</script>

</body>
</html>

